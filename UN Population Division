    <script type="text/javascript">
        function openFeedback(URL, sender, args) {
            var windowHeight = 355;

            //Get the reference to window
            var windowFeedback = $("#windowFeedback");

            //Create window object if not created yet
            if (!windowFeedback.data("kendoWindow")) {
                windowFeedback.kendoWindow({
                    width: "300px",
                    height: windowHeight + "px",
                    title: "Feedback",
                    content: URL,
                    modal: true,
                    resizable: false,
                    actions: ['close']
                });
            }
            else {
                windowFeedback.data("kendoWindow").refresh(URL);
            }

            //Move window to location where event happened when window was requested to be open (most of the times, mouse location when user clicked the link)
            var X = args.clientX + document.body.scrollLeft + document.documentElement.scrollLeft;
            var Y = args.clientY + document.body.scrollTop + document.documentElement.scrollTop - windowHeight;
            windowFeedback.closest(".k-window").css({
                top: Y,
                left: X
            });

            //Open the feedback window
            windowFeedback.data("kendoWindow").open();
        }

        //This is called from within the feedback window
        function closeFeedback() {
            $("#windowFeedback").data("kendoWindow").close();
        }
    </script>
    
