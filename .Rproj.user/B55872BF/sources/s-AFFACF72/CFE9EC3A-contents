#Get Data
Rock_Flip_Lab_EXP <- read_csv("~/Desktop/Dia_WD/Rock_Flip_EXP_Comb2.csv")

#Load Packages

library(readr)
library(lme4)
library(lmerTest)

Rock_Flip_Lab_EXP$location <- c(
  `Field` = "A",
  `Lab` = "B"
)

Rock_Flip_Lab_EXP %>%
  ggplot(aes(Orientation,Decrease, color= Orientation)) +
  scale_color_manual(values=c("gray30", "#56B4E9")) +
  theme_bw() +
  labs(y = expression(Delta * "  Abundance"), x = "")  +
  facet_grid(. ~ location ) +
  theme(legend.position="none") +
  theme(panel.grid.major = element_blank(), panel.grid.minor = element_blank()) +
  stat_summary(fun.mean=mean, geom="point", size = 2.5) +
  stat_summary(fun.data = mean_se, geom = "errorbar",width=.2) +
  theme(axis.text=element_text(size=12),
        axis.title=element_text(size=16,face="bold")) +
  theme(strip.text.x = element_text(size = 16, hjust = .05))

#One-Way Anova without random effects
Dlin.aov<- aov(Rock_Flip_Lab_EXP$Change_In_Abundance~Rock_Flip_Lab_EXP$Orientation*Rock_Flip_Lab_EXP$Location)
summary(Dlin.aov)
plot(TukeyHSD(Dlin.aov, ordered=TRUE))


#linear mixed effects model
model<-lmer(Change_In_Abundance ~ Orientation + (1| Location), data=Rock_Flip_Lab_EXP, REML=T)
summary(model)
anova(model)
rand(model)

#Validation
hist(residuals(model))
qqnorm(resid(model))
plot(fitted(model),residuals(model))
